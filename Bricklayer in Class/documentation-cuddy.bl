open Level_5;
build(10,10,10);

(*-------------------------------------------------*)


(* #### LINK TO BRICK COLORS ####

http://wintercoding.com/BricklayerWeb/legend_LEGO.html

*)

(*-------------------------------------------------*)

(* #### PUT ####

put (2,3,4) BLUE (0,0,0);

putHollow (4,6,8) [BLUE,GREEN] (0,0,0);

*)

(*-------------------------------------------------*)

(* #### LINE ####

line (10, 0,0) (10,10,0) RED;

*)


(*-------------------------------------------------*)

(* #### CUBE ####

This is a 5x5x5 cube placed at the origin

put(5,5,5) RED (0,0,0);

*)

(*-------------------------------------------------*)

(* #### MULTI COLORING ####

putMultibrick (2,3,4) [BLUE,GREEN] (0,0,0);

*)

(*-------------------------------------------------*)

(* #### SPHERE ####

sphere 10 [BLUE,GREEN] (20,20,20);

hollowSphere 10 10 [BLUE,GREEN] (20,20,20);


*)

(*-------------------------------------------------*)

(* #### RING ####

ringX 10 1 20 [BLUE,GREEN] (0,20,20);

*)

(*-------------------------------------------------*)

(* #### CONE ####

coneX 10 1 [BLUE,GREEN] (20,20,20);

coneNegX 10 1 [BLUE,GREEN] (20,20,20);

*)


(*-------------------------------------------------*)

(* #### RING ####

hollowCylinderX 10 20 [BLUE,GREEN] (0,20,20);

*)

(*-------------------------------------------------*)



show "cube";