open Level_4;

val length = 4;
val height = 8;
val width = 10;
val cubeSide = 8;

putMultibrick (2,3,4) [BLUE,GREEN] (10,0,0);

fun cube(x,y,z) = (
    put(cubeSide,cubeSide-2,cubeSide) TRANSPARENT (x,y,z);
    put(cubeSide-2,cubeSide-4,cubeSide-2) RED (x+1,y+1,z+1);
    (*** BLACK BRICK TO SHOW ORIGIN ***)
    put(1,1,1)  BLACK  (x,y,z)

);


